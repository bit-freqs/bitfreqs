{
  "name": "bitFreqs",
  "version": "1.0.0",
  "description": "game for global game jam 17",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test/index.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": ["babelify"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pietgeursen/bitFreqs"
  },
  "author": "pietgeursen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pietgeursen/bitFreqs/issues"
  },
  "homepage": "https://github.com/pietgeursen/bitFreqs",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
